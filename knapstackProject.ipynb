        "#Write your code here\n",
        "\n",
        "\n",
        "def knapdyn(values, weights, quantities, capacity):\n",
        "  \"\"\"\n",
        "    Bounded knapsack Dynamically.\n",
        "    Args:\n",
        "      values: list of values (len n)\n",
        "      weights: list of weights (len n), weights must be >= 0\n",
        "      quantities: list of non-negative integers (len n)\n",
        "      capacity: available capacity (>= 0)\n",
        "    Returns:\n",
        "      total_value\n",
        "      - total_value: float, maximum achievable value\n",
        "    Complexity:\n",
        "      Time:  Complexité: O(n * W * qtt_max) worst-case (où qtt_max est la borne sur les quantités parcourues).\n",
        "\n",
        "  \"\"\"\n",
        "  n = len(values)\n",
        "  if not (len(weights) == n and len(quantities) == n):\n",
        "    raise ValueError(\"values, weights and quantities must have the same length\")\n",
        "  if capacity < 0:\n",
        "    raise ValueError(\"capacity must be non-negative\")\n",
        "\n",
        "  dp = [[0] * (capacity + 1) for i in range(n + 1)]\n",
        "\n",
        "  # Remplissage de la table\n",
        "  for i in range(1, n + 1):\n",
        "    w_i = weights[i-1]\n",
        "    v_i = values[i-1]\n",
        "    c_i = quantities[i-1]\n",
        "    if w_i == 0:\n",
        "      for w in range(capacity + 1):\n",
        "        dp[i][w] = dp[i-1][w] + v_i*c_i\n",
        "      continue\n",
        "\n",
        "\n",
        "    for w in range(capacity + 1):\n",
        "      best = dp[i-1][w]\n",
        "\n",
        "      for k in range(1, c_i + 1):\n",
        "        if k>(w//w_i):\n",
        "          break\n",
        "        value = dp[i-1][w - k * w_i] + k * v_i\n",
        "        if value > best:\n",
        "          best = value\n",
        "      dp[i][w] = best\n",
        "\n",
        "  return dp[n][capacity]\n",
        "\n",
        "\n",
        "\"\"\"\n",
        "def print_dp_table(dp):\n",
        "    n = len(dp) - 1\n",
        "    W = len(dp[0]) - 1\n",
        "    header = [\"w\\\\i\"] + [str(i) for i in range(0, W+1)]\n",
        "    print(\"\\t\".join(header))\n",
        "    for i in range(0, n+1):\n",
        "        row = [f\"i={i}\"] + [str(dp[i][w]) for w in range(0, W+1)]\n",
        "        print(\"\\t\".join(row))\n",
        "\"\"\"\n",
        "'''\n",
