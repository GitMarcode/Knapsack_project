        "#Write your code here\n",
        "import time\n",
        "def knapfrac(values, weights, quantities, capacity):\n",
        "  \"\"\"\n",
        "    Fractional bounded knapsack.\n",
        "    Args:\n",
        "      values: list of values (len n)\n",
        "      weights: list of weights (len n), weights must be >= 0\n",
        "      quantities: list of non-negative integers (len n)\n",
        "      capacity: available capacity (>= 0)\n",
        "    Returns:\n",
        "      - total_value: float, maximum achievable value\n",
        "    Complexity:\n",
        "      Time: O(n log n) due to sort function\n",
        "  \"\"\"\n",
        "  n = len(values)\n",
        "  if not (len(weights) == n and len(quantities) == n):\n",
        "    raise ValueError(\"values, weights and quantities must have the same \")\n",
        "  if capacity <= 0:\n",
        "    return 0.0\n",
        "  # Build list of items with ratio\n",
        "  items = []\n",
        "  for i in range(len(values)):\n",
        "    w = weights[i]\n",
        "    if w < 0:\n",
        "      raise ValueError(\"weights must be non-negative\")\n",
        "    if quantities[i] < 0 or not isinstance(quantities[i], int):\n",
        "      raise ValueError(\"quantities must be non-negative integers\")\n",
        "    # ratio infinie si poids nul (priorité maximale)\n",
        "    ratio = float('inf') if w == 0 else values[i] / w\n",
        "    items.append((i, values[i], w, quantities[i], ratio))\n",
        "  # Sort by decreasing ratio\n",
        "  items.sort(key=lambda item: item[4], reverse=True)\n",
        "\n",
        "  remaining = capacity\n",
        "  total_value = 0.0\n",
        "\n",
        "  for item in items:\n",
        "    if remaining <= 0:\n",
        "      break\n",
        "    objet = item[0]\n",
        "    v = item[1]\n",
        "    w = item[2]\n",
        "    q = item[3]\n",
        "\n",
        "    if q == 0:\n",
        "      continue\n",
        "    if w == 0:\n",
        "      if v != 0:\n",
        "        total_value += v * q\n",
        "      continue\n",
        "\n",
        "    poid = w * q\n",
        "    if poid <= remaining:\n",
        "      total_value += v * q\n",
        "      remaining -= poid\n",
        "    else:\n",
        "      # take fractional part\n",
        "      f = remaining / w\n",
        "      total_value += v * f\n",
        "      remaining = 0.0\n",
        "      break\n",
        "\n",
        "  return total_value\n",
        "\n",
        "\n",
        "#values = [60, 100, 12,50]\n",
        "#weights = [10, 10, 30,10]\n",
        "#quantities = [2, 3, 2,3]\n",
