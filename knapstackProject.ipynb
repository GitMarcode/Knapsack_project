        "    quantities_restantes = quantities.copy()\n",
        "    best_value = explore(0,0,0,quantities_restantes,values,weights,capacity,best_value)\n",
        "    return best_value\n",
        "\n",
        "#exmpores a search tree of potential solutions :\n",
        "\n",
        "def explore(i,current_value,current_weight,quantities_restantes,values,weights,capacity,best_value):\n",
        "  if i == len(values):\n",
        "    if current_value > best_value:\n",
        "      best_value = current_value\n",
        "    return best_value\n",
        "\n",
        "  #capacity ne peut pas contenir plus d'objet !\n",
        "  capacity_left = capacity - current_weight\n",
        "  if capacity_left<=0: #normalement never <0\n",
        "    if current_value > best_value:\n",
        "      best_value = current_value\n",
        "    return best_value\n",
        "\n",
        "  #Calcul de l'upper bound de la node\n",
